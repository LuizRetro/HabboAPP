{
  "name": "Seu Hotel",
  "productName": "Seu Hotel",
  "settings": {
    "hotelName": "Seu Hotel",
    "url": "https://habbo.com.br",
    "useFrame": true,
    "backgroundColor": "#e3e3db",
    "hideMenu": false
  },
  "version": "2.0.0",
  "description": "Aplicativo desktop para o Habbo",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package-mac": "electron-forge package --arch=x64 --platform=darwin",
